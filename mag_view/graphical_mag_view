
datas = fscanfMat("C:\Users\Toshiba\Documents\Vincent MAIRE\MPU\Mag_view\datas.txt")

n = size(datas)(1)
for i=1:n
    if datas(i,1) == 1 && datas(i,2) == 2 && datas(i,3) == 3
        break
    end
    datas_not_cal(i,1:3) = datas(i,1:3)
end

index = 0

if i < n 
    for j=(i+1):n
        index = index + 1
        datas_cal(index,1:3) = datas(j,1:3)
    end
end

disp(datas_not_cal)
disp(datas_cal)
